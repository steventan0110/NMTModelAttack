#!/home/steven/Documents/GITHUB/temp/COMET/comet/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'unbabel-comet','console_scripts','comet'
__requires__ = 'unbabel-comet'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('unbabel-comet', 'console_scripts', 'comet')()
    )
